{
  "component": {
    "alert": {
      "padding": {
        "x": {
          "value": "{space.static.250}",
          "type": "spacing"
        },
        "y": {
          "value": "{space.static.250}",
          "type": "spacing"
        }
      },
      "border-radius": {
        "value": "{borderRadius.lg}",
        "type": "borderRadius"
      },
      "gap": {
        "value": "{space.static.200}",
        "type": "spacing"
      },
      "default": {
        "value": {
          "horizontalPadding": "{component.alert.padding.x}",
          "verticalPadding": "{component.alert.padding.y}",
          "itemSpacing": "{component.alert.gap}",
          "borderRadius": "{component.alert.border-radius}"
        },
        "type": "composition"
      },
      "dismissible": {
        "value": {
          "verticalPadding": "{component.alert.padding.y}",
          "itemSpacing": "{component.alert.gap}",
          "borderRadius": "{component.alert.border-radius}",
          "paddingLeft": "{component.alert.padding.x}",
          "paddingRight": "{component.alert.dismiss.padding.right}"
        },
        "type": "composition"
      },
      "dismiss": {
        "padding": {
          "right": {
            "value": "{space.static.600}",
            "type": "spacing"
          }
        }
      }
    }
  }
}